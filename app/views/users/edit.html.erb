<h1>会員情報編集</h1>
<%= form_for @user, url: "/users/#{@user.id}" do |f| %>  
    <h4>名前(漢字)</h4>
    <h4>(姓)</h4>
    <%= f.text_field :last_name %> 
    <h4>名</h4>
    <%= f.text_field :first_name %>
    <h4>名前(カナ)</h4>
    <h4>(セイ)</h4>
    <%= f.text_field :last_name_kana %> 
    <h4>メイ</h4>
    <%= f.text_field :first_name_kana %>
    <h4>電話番号</h4>
    <%= f.text_field :phone %>
    <h4>メールアドレス</h4>
    <%= f.text_field :email %>
    <h4>既定住所</h4>
    <%= f.text_field :postal_code %>
    <%= f.text_field :adress %>
    <%= f.submit '更新' %>
<% end %>

